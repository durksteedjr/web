{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "down": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "seed": {
      "cache": false
    },
    "typecheck": {
      "cache": true,
      "dependsOn": ["^typecheck"]
    }
  }
}
